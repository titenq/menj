{
  "name": "menj",
  "version": "0.1.0",
  "description": "MENJ CLI - Scaffold for API with NodeJS, Express, MongoDB and JWT.",
  "author": "TitenQ <titenq@gmail.com>",
  "contributors": [
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/titenq/menj.git"
  },
  "homepage": "https://github.com/titenq/menj",
  "keywords": [
    "express",
    "nodejs",
    "mongodb",
    "mongoose",
    "jwt",
    "api"
  ],
  "private": false,
  "bin": {
    "menj": "bin/menj"
  },
  "preferGlobal": true,
  "scripts": {
    "format": "prettier --write **/*.{js,json} && standard --fix",
    "lint": "standard",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "src",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "latest"
  },
  "devDependencies": {
    "standard": "^12.0.1",
    "prettier": "^1.12.1",
    "jest": "^24.1.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
